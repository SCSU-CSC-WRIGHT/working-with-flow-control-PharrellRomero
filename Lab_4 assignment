#Lab 1:Guess the Number
import random
num= random.randint(1,10)
attempts=3
count=1
while count <= attempts:
    guess= int(input("Guess a number between 1-10: "))
    if guess>num:
        print("Too high")
    elif guess<num:
        print("Too low")
    else:
        print("Correct")
        break

#Lab 2:Countdown Timer
num=int(input("Put a number to start a countdown: "))
while num != 0:
    print(num)
    num-=1
#Lab3: Sum of Numbers
total=0
num=int(input("Enter a number: "))
for i in range(1,num+1):
    total += i
    print(total)

